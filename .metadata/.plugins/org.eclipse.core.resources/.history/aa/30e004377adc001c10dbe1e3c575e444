import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Conta cc = new ContaCorrente();
		
		
		Conta poupanca = new ContaPoupança();
		
		
		Scanner scan = new Scanner(System.in);
		int a;
		a = scan.nextInt();
		cc.depositar(a);
		
		//cc.transferir(a, poupanca);
		cc.imprimirExtrato();
		poupanca.imprimirExtrato();
		
	}

}
